	"exif": {
		"camera_model": {
			"exif_field": "EXIF:Model",
			"format": "%s",
			"hashtags": {
				"Canon EOS M50": "#canon #eos #eosM50 #canonm50 #m50 #canoneosm50 #캐논 #캐논M50 #캐논EOSM50 #색감",
				"Canon EOS R6": "#canon #eos #eosR6 #canonR6 #R6 #canoneosR6 #캐논 #캐논R6 #캐논EOSR6 #색감"
			}
		},
		"lens": {
			"exif_field": "EXIF:LensModel",
			"format": "%s",
			"hashtags": {
				"RF35mm F1.8 MACRO IS STM": "#rf35mm #rf35mmmacro #rf35mm18"
				
			}
		},
		"focal_length": {
			"exif_field": "EXIF:FocalLength",
			"format": "%smm",
			"hashtag": "#%smm"
		},
		"aperture": {
			"exif_field": "Composite:Aperture",
			"format": "f/%s"
		},
		"shutter_speed": {
			"exif_field": "EXIF:ExposureTime",
			"format": "%ss",
			"conditional_hashtags": {
				">= 0.5": "#longexposure #장노출"
			}
		},
		"iso": {
			"exif_field": "EXIF:ISO",
			"format": "ISO %s"
		},
		"white_balance": {
			"exif_fields": ["MakerNotes:ColorTemperature", "MakerNotes:WBShiftAB", "MakerNotes:WBShiftGM"],
			"format": "WB %1K AB%2 GM%3"
		},
		"focus_distance": {
			"exif_field": "MakerNotes:FocusDistanceLower"
		}
		
	},

	"images_basedir": "/home/kiyoon/project/camera-tools/instagram_tools"
}
